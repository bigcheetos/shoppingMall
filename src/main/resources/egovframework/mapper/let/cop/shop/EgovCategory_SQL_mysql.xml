<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CategoryDAO">
	
	<select id="selectCategoryAll" resultMap="categoryResultMap" >
			SELECT 
				CATEGORY_SEQ,
				CATEGORY_NAME,
				CATEGORY_PARENT_SEQ,
				CATEGORY_ORDER
			FROM
				CATEGORY			
	</select>
	
	<resultMap id="categoryResultMap" type="egovframework.let.cop.shop.service.CategoryVO">
		<result property="categorySeq" column="CATEGORY_SEQ"/>
		<result property="categoryName" column="CATEGORY_NAME"/>
		<result property="categoryParentSeq" column="CATEGORY_PARENT_SEQ"/>
		<result property="categoryOrder" column="CATEGORY_ORDER"/>
	</resultMap>
</mapper>